{
  "name": "@condict/dev-server",
  "version": "0.1.5",
  "description": "Thin wrapper around webpack-dev-server that injects Condict-specific config.",
  "repository": "github:arimah/condict",
  "author": "Alice Heurlin <alice.s.heurlin@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node ./dev/server",
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "compile": "tsc",
    "fix:types": "rimraf node_modules/@types/react-native",
    "build": "npm run fix:types && npm run clean && npm run compile",
    "prepublishOnly": "npm install && npm run build",
    "lint": "eslint --ext .ts,.tsx src/ dev/"
  },
  "dependencies": {
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^3.2.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.4.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "chalk": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "rimraf": "^3.0.0",
    "styled-components": "^5.0.0-rc.2",
    "styled-tools": "^1.7.1",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
