{
  "name": "@condict/dev-server",
  "version": "0.2.0",
  "description": "Thin wrapper around webpack-dev-server that injects Condict-specific config.",
  "repository": "github:arimah/condict",
  "author": "Alice Heurlin <alice.s.heurlin@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node ./dev/server",
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "compile": "tsc",
    "fix:types": "rimraf node_modules/@types/react-native",
    "build": "npm run fix:types && npm run clean && npm run compile",
    "prepublishOnly": "npm install && npm run build",
    "lint": "eslint --ext .ts,.tsx src/ dev/"
  },
  "dependencies": {
    "html-replace-webpack-plugin": "^2.5.6",
    "html-webpack-plugin": "^4.3.0",
    "ts-loader": "^7.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@condict/eslint-plugin": "^0.2.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "@types/webpack": "^4.41.17",
    "@types/webpack-dev-server": "^3.11.0",
    "chalk": "^4.1.0",
    "eslint": "^7.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
