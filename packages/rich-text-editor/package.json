    "slate-history": "^0.58.4",
    "slate-react": "^0.58.4"
  },
  "devDependencies": {
    "@condict/dev-server": "^0.2.0",
    "@condict/eslint-plugin": "^0.2.0",
    "@condict/ui": "^0.9.0",
    "@types/react": "^16.9.38",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.0",
    "platform": "^1.3.5",
    "react": "^16.13.1",
