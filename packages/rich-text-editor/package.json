  "description": "Rich-text editor components for the Condict admin.",
  "repository": "github:arimah/condict",
  "author": "Alice Heurlin <alice.s.heurlin@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
