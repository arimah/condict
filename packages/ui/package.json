{
  "name": "@condict/ui",
  "version": "0.7.10",
  "description": "Shared UI components for Condict.",
  "repository": "github:arimah/condict",
  "author": "Alice Heurlin <alice.s.heurlin@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "compile": "tsc -p tsconfig.build.json",
    "fix:types": "rimraf node_modules/@types/react-native",
    "build": "npm run fix:types && npm run clean && npm run compile",
    "dev": "cross-env NODE_ENV=development node dev/server.js",
    "prepublishOnly": "npm install && npm run build",
    "lint": "eslint --ext .ts,.tsx src/ dev/"
  },
  "dependencies": {
    "@condict/a11y-utils": "^0.1.6",
    "@condict/gen-id": "^0.1.1",
    "@condict/platform": "^0.1.0",
    "autosize-input": "^1.0.2",
    "mdi-react": "^6.2.0",
    "memoize-one": "^5.1.1"
  },
  "devDependencies": {
    "@condict/dev-server": "^0.1.5",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "platform": "^1.3.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.0",
    "styled-components": "^5.0.0-rc.2",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "platform": "^1.3.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-is": "^16.12.0",
    "styled-components": "^5.0.0-rc.2"
  }
}
