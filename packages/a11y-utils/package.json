{
  "name": "@condict/a11y-utils",
  "version": "0.1.6",
  "description": "Accessibility utils for Condict, primarily its UI components.",
  "repository": "github:arimah/condict",
  "author": "Alice Heurlin <alice.s.heurlin@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "rimraf dist",
    "compile": "tsc",
    "fix:types": "rimraf node_modules/@types/react-native",
    "build": "npm run fix:types && npm run clean && npm run compile",
    "prepublishOnly": "npm install && npm run build",
    "lint": "eslint --ext .ts,.tsx src/"
  },
  "devDependencies": {
    "@types/react": "^16.8.24",
    "@types/styled-components": "4.1.8",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.2"
  },
  "peerDependencies": {
    "styled-components": "^5.0.0-rc.2"
  }
}
