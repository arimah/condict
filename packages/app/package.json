{
  "name": "@condict/app",
  "productName": "Condict",
  "private": true,
  "description": "The main Electron app for Condict.",
  "repository": "github:arimah/condict",
  "author": "Alice Heurlin <alice.s.heurlin@gmail.com>",
  "license": "MIT",
  "main": "dist/main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "start:prod": "cross-env NODE_ENV=production electron .",
    "build": "npm run -s build:graphql && npm run -s build:main",
    "build:main": "rollup -c",
    "build:graphql": "condict-graphql-typer --schema-dir ../../graphql-schema --target=client --src=./src/renderer/ --defs=./src/renderer/graphql-shared.ts",
    "build:native": "electron-rebuild -f -w bcrypt,better-sqlite3,@condict/server",
    "watch": "rollup -c --watch"
  },
  "dependencies": {
    "@condict/platform": "^0.1.0",
    "@condict/server": "^0.1.0",
    "@condict/ui": "^0.1.0",
    "apollo-server": "^2.23.0",
    "better-sqlite3": "^7.3.0",
    "bcrypt": "^5.0.1",
    "graphql": "^15.5.0",
    "immer": "^9.0.1",
    "mdi-react": "^7.5.0",
    "memoize-one": "^5.1.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "shallowequal": "^1.1.0",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@condict/graphql-typer": "^0.1.0",
    "@types/node": "^12.12.47",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/shallowequal": "^1.1.1",
    "@types/styled-components": "^5.1.9",
    "cross-env": "^7.0.3",
    "electron": "^12.0.9",
    "electron-rebuild": "^2.3.5",
    "node-abi": "^2.26.0",
    "rimraf": "^3.0.2"
  }
}
